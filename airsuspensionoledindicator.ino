void setup() {
  Serial.begin(9600);
  oled.init();        // инициализация
  oled.clear();       // очистка
  if(isBuffered)
  {
    oled.update();
  }

  values[0].pin = A0;
  values[1].pin = A1;
  values[2].pin = A2;
  values[3].pin = A3;
  for(int i = 0; i < 4; ++i)
  {  
     pinMode(values[i].pin, INPUT_PULLUP);
  }

  Serial.println("Started. Used pins: A0 - A3");

  pinMode(LED_BUILTIN, OUTPUT);
}

void loop() {
  unsigned long nowT = millis();
  for(int i = 0; i < 4; ++i)
  {    
    if(nowT - values[i].prevTime >= readDelay)
    {  
      bool active = analogRead(values[i].pin) * 5.0 / 1024 < 1;
      if(active != values[i].current)
      {
        values[i].current = active;
        values[i].prevTime = nowT;
        Serial.print("Pin A");
        Serial.print(i);
        Serial.print(" changed state to ");        
        Serial.println(active ? "true" : "false");        
      }
    }
  }

  byte stateImage = values[0].current ? 0 : 4;
  stateImage = values[1].current ? 1 : stateImage;
  stateImage = values[2].current ? 2 : stateImage;
  stateImage = values[3].current ? 3 : stateImage;

  if(currentImage != stateImage)
  {
    oled.clear();

    // рисование
    switch(stateImage)
    {
      case 0:
        oled.drawBitmap(0, 0, bitmap_1, 128, 96, BITMAP_NORMAL, BUF_ADD);
        break;
      case 1:
        oled.drawBitmap(0, 0, bitmap_2, 128, 96, BITMAP_NORMAL, BUF_ADD);
        break;
      case 2:
        oled.drawBitmap(0, 0, bitmap_3, 128, 96, BITMAP_NORMAL, BUF_ADD);
        break;
      case 3:
        oled.drawBitmap(0, 0, bitmap_4, 128, 96, BITMAP_NORMAL, BUF_ADD);
        break;
    }

    if(isBuffered)
    {
      oled.update();
    }

    if(stateImage < 4)
    {
      digitalWrite(LED_BUILTIN, HIGH);
    }
    else
    {
      digitalWrite(LED_BUILTIN, LOW);
    }

    currentImage = stateImage;
    Serial.println("Redrawed");    
  }
}
